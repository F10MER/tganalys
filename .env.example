# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_from_botfather

# AI Provider Configuration
# Выберите провайдера: openai, openrouter, agentrouter
AI_PROVIDER=openai

# OpenAI Configuration (если AI_PROVIDER=openai)
OPENAI_API_KEY=your_openai_api_key

# OpenRouter Configuration (если AI_PROVIDER=openrouter)
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_MODEL=anthropic/claude-3.5-sonnet
# Другие популярные модели:
# anthropic/claude-3-opus
# google/gemini-pro-1.5
# meta-llama/llama-3.1-70b-instruct
# openai/gpt-4o

# AgentRouter Configuration (если AI_PROVIDER=agentrouter)
AGENTROUTER_API_KEY=your_agentrouter_api_key

# Transcription Provider Configuration
# Выберите провайдера: whisper_local, openai, assemblyai
TRANSCRIPTION_PROVIDER=whisper_local

# Local Whisper Configuration (если TRANSCRIPTION_PROVIDER=whisper_local)
WHISPER_API_URL=http://whisper:9000

# AssemblyAI Configuration (если TRANSCRIPTION_PROVIDER=assemblyai)
# ASSEMBLYAI_API_KEY=your_assemblyai_api_key

# Примечание: Для TRANSCRIPTION_PROVIDER=openai используется OPENAI_API_KEY сверху

# Database Configuration
DATABASE_URL=postgresql://telegram_user:CHANGE_PASSWORD@postgres:5432/telegram_bot

# Bot Configuration
ADMIN_USER_IDS=123456789,987654321
